<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.adhiwie.moodjournal">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <!-- <uses-permission android:name="android.permission.READ_CONTACTS" /> -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="com.google.android.gms.permission.ACTIVITY_RECOGNITION" />

    <!-- <uses-permission android:name="android.permission.READ_CALL_LOG" /> -->
    <!-- <uses-permission android:name="android.permission.READ_SMS" /> -->
    <!-- <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" /> -->
    <!-- <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> -->
    <!-- <uses-permission -->
    <!-- android:name="android.permission.PACKAGE_USAGE_STATS" -->
    <!-- tools:ignore="ProtectedPermissions" /> -->
    <!-- <uses-permission android:name="android.permission.READ_PHONE_STATE" /> -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ConsentActivity"
            android:label="@string/title_activity_consent" />
        <activity
            android:name=".questionnaire.personality.PersonalityTestActivity"
            android:label="@string/title_activity_personality_test"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".questionnaire.wellbeing.WellBeingQuestionnaireActivity"
            android:label="@string/title_activity_wellbeing_questionnaire"
            android:screenOrientation="portrait" />
        <activity
            android:name=".questionnaire.mood.MoodQuestionnaireActivity"
            android:label="@string/title_activity_mood_questionnaire"
            android:screenOrientation="portrait" />
        <activity
            android:name=".user.permission.AccessibilityServicePermissionActivity"
            android:label="@string/title_activity_accessibility_permission" />
        <activity
            android:name=".user.permission.AppUsagePermissionActivity"
            android:label="@string/title_activity_app_usage_permission" />
        <activity
            android:name=".user.permission.NSLPermissionActivity"
            android:label="@string/title_activity_nsl_permission" />
        <activity
            android:name=".plan.PlanActivity"
            android:label="@string/title_activity_create_plan" />
        <activity
            android:name=".report.MoodReportActivity"
            android:label="@string/title_activity_mood_data" />
        <activity
            android:name=".questionnaire.goalcommitment.GCSActivity"
            android:label="@string/title_activity_gcs_test" />
        <activity
            android:name=".questionnaire.posttest.SRBAIActivity"
            android:label="@string/title_activity_srbai_test" />
        <activity android:name=".questionnaire.mood.ReminderActivity" />

        <service
            android:name=".sensor.push.AccessibilitySensor"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/accessibility_service_config" />
        </service>
        <service
            android:name=".sensor.push.ActivitySensor"
            android:exported="false"
            android:label="@string/app_name" />
        <service
            android:name=".sensor.push.LocationSensor"
            android:exported="false"
            android:label="@string/app_name" />
        <service
            android:name=".notification.NotificationMonitorService"
            android:label="@string/app_name"
            android:permission="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE">
            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService" />
            </intent-filter>
        </service>

        <receiver android:name=".sensor.push.NetworkStateSensor">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
        <receiver android:name=".sensor.push.PowerConnectionSensor">
            <intent-filter>
                <action android:name="android.intent.action.ACTION_POWER_CONNECTED" />
                <action android:name="android.intent.action.ACTION_POWER_DISCONNECTED" />
            </intent-filter>
        </receiver>
        <receiver android:name=".sensor.push.RingerModeSensor">
            <intent-filter>
                <action android:name="android.media.RINGER_MODE_CHANGED" />
            </intent-filter>
        </receiver>

    </application>

</manifest>